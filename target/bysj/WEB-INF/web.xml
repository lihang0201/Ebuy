<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
  <servlet>
    <servlet-name>UserServlet</servlet-name>
    <servlet-class>User.UserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserServlet</servlet-name>
    <url-pattern>/UserServlet</url-pattern>
  </servlet-mapping>


  <servlet>
    <servlet-name>VerifyServlet</servlet-name>
    <servlet-class>User.VerifyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>VerifyServlet</servlet-name>
    <url-pattern>/VerifyServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ElectronicsServlet</servlet-name>
    <servlet-class>Waste.Electronics.ElectronicsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ElectronicsServlet</servlet-name>
    <url-pattern>/ElectronicsServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>FoodServlet</servlet-name>
    <servlet-class>Waste.Food.FoodServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FoodServlet</servlet-name>
    <url-pattern>/FoodServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ClothesServlet</servlet-name>
    <servlet-class>Waste.Clothes.ClothesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ClothesServlet</servlet-name>
    <url-pattern>/ClothesServlet</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>OthersServlet</servlet-name>
    <servlet-class>Waste.Others.OthersServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OthersServlet</servlet-name>
    <url-pattern>/OthersServlet</url-pattern>
  </servlet-mapping>



</web-app>